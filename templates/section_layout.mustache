<li id="section-{{section.section}}"
    class="section section-type-{{sectiontype}} main clearfix {{sectionstyle}}"
    role="region"
    aria-labelledby="sectionid-{{section.id}}-title"
    data-sectionid="{{section.section}}"
    data-sectionreturnid="{{sectionreturn}}">

    <div class="left side">
        {{{leftcontent}}}
    </div>

    <div class="right side">
        {{{rightcontent}}}
    </div>

    <div class="content">
        {{$content}}

        {{/content}}
    </div>
</li>

{{#js}}
    require(['format_designer/designer_section'], function(DesignerSection) {
        console.log("HERE");
        var ds = new DesignerSection('#section-{{section.section}}', {{courseid}});
    });
{{/js}}